
config 'global' 'automount'
	option 'from_fstab' '1'
	option 'anon_mount' '1'

config 'global' 'autoswap'
	option 'from_fstab' '1'
	option 'anon_swap' '0'

config 'swap'
	option 'device' '/mnt/sda1/swapfiles'
	option 'enabled' '1'

config 'mount'
	option 'uuid' 'F4E2-D9A5'
	option 'enabled' '1'
	option 'fstype' 'vfat'
	option 'device' '/dev/sdb1'
	option 'target' '/mnt/sda1'

